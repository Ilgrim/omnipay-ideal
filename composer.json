{
    "name": "bamarni/omnipay-ideal",
    "description": "iDeal payment gateway for Omnipay",
    "type": "omnipay-gateway",
    "keywords": ["omnipay", "payment", "gateway", "ideal", "provider"],
    "license": "Unlicense",
    "authors": [
        {
            "name": "Bilal Amarni",
            "email": "bilal.amarni@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Bamarni\\Omnipay\\Ideal\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Bamarni\\Omnipay\\Ideal\\Tests\\" : "tests/",
            "Omnipay\\" : "tests/omnipay-test-files/"
        }
    },
    "require": {
        "php": ">=5.4",
        "omnipay/omnipay": "1.*"
    },
    "require-dev": {
        "guzzle/plugin-mock": "~3.1",
        "mockery/mockery": "~0.7",
        "silex/silex": "1.0.*@dev",
        "squizlabs/php_codesniffer": "~1.4.4",
        "twig/twig": "~1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
